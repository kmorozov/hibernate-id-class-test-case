<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="kmorozov.bean.Meal" table="meal">
        <composite-id class="kmorozov.bean.MealId" mapped="true">
        	<key-property name="ham" />
            <key-property name="eggs" />
        </composite-id>
    </class>
</hibernate-mapping>
